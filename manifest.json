{
  "v2": {
    "transposit": {
      "dependencies": [{
        "name": "transposit/slack:v2.*.*",
        "alias": "slack",
        "title": "Slack",
        "description": "Authorize this connector if you want to use the Slack API."
      }, {
        "name": "transposit/slack_webhook:v2.*.*",
        "alias": "slack_webhook"
      }, {
        "name": "transposit/aws_s3:v3.*.*",
        "alias": "aws_s3"
      }, {
        "name": "transposit/gcp_bigquery:v1.*.*",
        "alias": "gcp_bigquery"
      }, {
        "name": "transposit/google_sheets:v1.*.*",
        "alias": "google_sheets"
      }],
      "operations": [{
        "id": "webhook",
        "function": {
          "parameters": [{
            "name": "http_event",
            "default": {
              "text": "{   \"query_parameters\": {     \"api_key\": \"XXX\"   },   \"http_method\": \"POST\",   \"parsed_body\": {     \"channel_name\": \"XXX\",     \"user_id\": \"UXXX\",     \"user_name\": \"iggy\",     \"trigger_id\": \"XXX\",     \"team_domain\": \"my_domain\",     \"team_id\": \"TXXX\",     \"text\": \"\",     \"channel_id\": \"DXXX\",     \"command\": \"/test\",     \"token\": \"XXX\",     \"response_url\": \"https://hooks.slack.com/commands/TXXX/XXX/XXX\"   },   \"body\": \"token=XXX&team_id=TXXX&team_domain=my_domain&channel_id=DXXX&channel_name=XXX&user_id=UXXX&user_name=iggy&command=%2Ftest&text=&response_url=https%3A%2F%2Fhooks.slack.com%2Fcommands%2FTXXX%2FXXX%2FXXX&trigger_id=XXX\" }"
            },
            "type": "OBJECT"
          }],
          "js": "webhook.js",
          "lang": "js"
        },
        "access": {
          "requireApiKey": true,
          "webhook": true
        }
      }, {
        "id": "get_slack_message",
        "function": {
          "parameters": [{
            "name": "user",
            "type": "STRING"
          }],
          "js": "get_slack_message.js",
          "lang": "js"
        }
      }, {
        "id": "pipeline_top",
        "function": {
          "parameters": [{
            "name": "file_url",
            "description": "file location",
            "type": "STRING",
            "required": true
          }],
          "js": "pipeline_top.js",
          "lang": "js"
        },
        "requestTimeoutMillis": 300000
      }, {
        "id": "load_file",
        "function": {
          "parameters": [{
            "name": "file_url",
            "type": "STRING",
            "required": true
          }],
          "js": "load_file.js",
          "lang": "js"
        }
      }, {
        "id": "get_object",
        "tql": {
          "parameters": [{
            "name": "bucket",
            "type": "STRING",
            "required": true
          }, {
            "name": "key",
            "type": "STRING",
            "required": true
          }],
          "tql": "SELECT * FROM aws_s3.get_object\n  WHERE Bucket=@bucket\n  AND Key=@key"
        }
      }, {
        "id": "execute_dw_query",
        "tql": {
          "tql": "SELECT rows FROM gcp_bigquery.execute_query\n  WHERE projectId='sales-data-pipeline'\nAND $body=(SELECT {\n    'query' : 'SELECT * FROM default.orderdata',\n  })\n\n"
        }
      }, {
        "id": "get_existing_records",
        "function": {
          "js": "lookup_dw_rows.js",
          "lang": "js"
        }
      }, {
        "id": "add_sales_rep",
        "function": {
          "parameters": [{
            "name": "data",
            "type": "ARRAY",
            "required": true
          }],
          "js": "add_sales_rep.js",
          "lang": "js"
        }
      }, {
        "id": "add_inventory",
        "function": {
          "parameters": [{
            "name": "data",
            "type": "ARRAY",
            "required": true
          }],
          "js": "add_inventory.js",
          "lang": "js"
        }
      }, {
        "id": "add_inventory_warning",
        "function": {
          "parameters": [{
            "name": "data",
            "type": "ARRAY",
            "required": true
          }],
          "js": "add_inventory_warning.js",
          "lang": "js"
        }
      }, {
        "id": "generate_region_totals",
        "function": {
          "parameters": [{
            "name": "data",
            "type": "ARRAY"
          }],
          "js": "generate_region_totals.js",
          "lang": "js"
        }
      }, {
        "id": "ship_totals_to_google_sheets",
        "function": {
          "parameters": [{
            "name": "totals",
            "type": "ARRAY",
            "required": true
          }],
          "js": "ship_totals_to_google_sheets.js",
          "lang": "js"
        }
      }, {
        "id": "update_sheet_values",
        "tql": {
          "parameters": [{
            "name": "spreadsheet_id",
            "type": "STRING",
            "required": true
          }, {
            "name": "range",
            "type": "STRING",
            "required": true
          }, {
            "name": "values",
            "type": "ARRAY",
            "required": true
          }],
          "tql": "SELECT * FROM google_sheets.update_sheet_values\n  WHERE range=@range\n  AND spreadsheetId=@spreadsheet_id\n  AND valueInputOption = 'RAW'\n  AND $body=(SELECT {\n    'majorDimension' : 'COLUMNS', -- see https://www.transposit.com/docs/references/connectors/google-sheets-documentation/#update_sheet_values\n    'values' : @values,\n    'range' : @range,\n  })\n"
        }
      }, {
        "id": "execute_dw_insert",
        "tql": {
          "parameters": [{
            "name": "region",
            "type": "STRING",
            "required": true
          }, {
            "name": "country",
            "type": "STRING",
            "required": true
          }, {
            "name": "item_type",
            "type": "STRING",
            "required": true
          }, {
            "name": "sales_channel",
            "type": "STRING",
            "required": true
          }, {
            "name": "order_priority",
            "type": "STRING",
            "required": true
          }, {
            "name": "order_id",
            "type": "INTEGER",
            "required": true
          }, {
            "name": "ship_date",
            "type": "STRING",
            "required": true
          }, {
            "name": "units_sold",
            "type": "INTEGER",
            "required": true
          }, {
            "name": "unit_price",
            "type": "NUMBER",
            "required": true
          }, {
            "name": "total_revenue",
            "type": "NUMBER",
            "required": true
          }, {
            "name": "unit_cost",
            "type": "NUMBER",
            "required": true
          }, {
            "name": "total_cost",
            "type": "NUMBER",
            "required": true
          }, {
            "name": "total_profit",
            "type": "NUMBER",
            "required": true
          }, {
            "name": "order_date",
            "type": "STRING"
          }],
          "tql": "SELECT * FROM gcp_bigquery.execute_query\n  WHERE projectId='sales-data-pipeline'\nAND $body=(SELECT {\n    'useLegacySql' : false,\n    'query' : 'INSERT into `default.orderdata` (Region, Country, Item_Type, Sales_Channel, Order_Priority, Order_Date, Order_ID, Ship_Date, Units_Sold, Unit_Price, Unit_Cost, Total_Revenue, Total_Cost, Total_Profit) VALUES (' \n           +\" '\"+ @region+\"',\"\n           +\" '\"+ @country+\"',\"\n           +\" '\"+ @item_type+\"',\"\n           +\" '\"+ @sales_channel+\"',\"\n           +\" '\"+ @order_priority+\"',\"\n           +\" '\"+ @order_date+\"',\"\n           +\" \"+ @order_id+\",\"\n           +\" '\"+ @ship_date+\"',\"\n           +\" \"+ @units_sold+\",\"\n           +\" \"+ @unit_price+\",\"\n           +\" \"+ @unit_cost+\",\"\n           +\" \"+ @total_revenue+\",\"\n           +\" \"+ @total_cost+\",\"\n           +\" \"+ @total_profit\n           + ')',\n  })\n-- \n  -- {\n    -- \"Region\": @region,\n    -- \"Country\": @country,\n    -- \"Item_Type\": @item_type,\n    -- \"Sales_Channel\": @sales_channel,\n    -- \"Order_Priority\": @order_priority,\n    -- \"Order_Date\": @order_date,\n    -- \"Order_ID\": @order_id,\n    -- \"Ship_Date\": @ship_date,\n    -- \"Units_Sold\": @units_sold,\n    -- \"Unit_Price\": @unit_price,\n    -- \"Unit_Cost\": @unit_cost,\n    -- \"Total_Revenue\": @total_revenue,\n    -- \"Total_Cost\": @total_cost,\n    -- \"Total_Profit\": @total_profit\n  -- },"
        }
      }, {
        "id": "ship_new_records",
        "function": {
          "parameters": [{
            "name": "data",
            "type": "ARRAY",
            "required": true
          }],
          "js": "ship_new_records.js",
          "lang": "js"
        }
      }, {
        "id": "execute_dw_insert_1",
        "tql": {
          "parameters": [{
            "name": "region",
            "type": "STRING",
            "required": true
          }, {
            "name": "country",
            "type": "STRING",
            "required": true
          }, {
            "name": "item_type",
            "type": "STRING",
            "required": true
          }, {
            "name": "sales_channel",
            "type": "STRING",
            "required": true
          }, {
            "name": "order_priority",
            "type": "STRING",
            "required": true
          }, {
            "name": "order_id",
            "type": "STRING",
            "required": true
          }, {
            "name": "ship_date",
            "type": "STRING",
            "required": true
          }, {
            "name": "units_sold",
            "type": "STRING",
            "required": true
          }, {
            "name": "unit_price",
            "type": "STRING",
            "required": true
          }, {
            "name": "total_revenue",
            "type": "STRING",
            "required": true
          }, {
            "name": "unit_cost",
            "type": "STRING",
            "required": true
          }, {
            "name": "total_cost",
            "type": "STRING",
            "required": true
          }, {
            "name": "total_profit",
            "type": "STRING",
            "required": true
          }, {
            "name": "order_date",
            "type": "STRING"
          }],
          "tql": "SELECT * FROM gcp_bigquery.execute_query\n  WHERE projectId='sales-data-pipeline'\n  \nAND $body=(SELECT {\n    'query' : \"insert into `default.orderdata` (Region) VALUES ('a')\",\n    'useLegacySql' : false\n});"
        }
      }, {
        "id": "execute_query",
        "tql": {
          "tql": "SELECT * FROM gcp_bigquery.execute_query\n  WHERE projectId='<required_string>'\n  AND $body=(SELECT {\n    'dryRun' : <boolean>,\n    'parameterMode' : '<string>',\n    'useQueryCache' : <boolean>,\n    'queryParameters' : [ {\n      'parameterType' : {\n        'arrayType' : '<string>',\n        'type' : '<string>',\n        'structTypes' : [ {\n          'name' : '<string>',\n          'description' : '<string>',\n          'type' : '<string>'\n        } ]\n      },\n      'name' : '<string>',\n      'parameterValue' : {\n        'structValues' : {\n          '<string>' : { }\n        },\n        'value' : '<string>',\n        'arrayValues' : [ { } ]\n      }\n    } ],\n    'kind' : '<string>',\n    'maxResults' : <integer>,\n    'timeoutMs' : <integer>,\n    'preserveNulls' : <boolean>,\n    'query' : '<string>',\n    'location' : '<string>',\n    'defaultDataset' : {\n      'datasetId' : '<string>',\n      'projectId' : '<string>'\n    },\n    'useLegacySql' : <boolean>\n  })\n\n/*\n * For sample code and reference material, visit\n * https://www.transposit.com/docs/get-started/sql-quickstart\n */"
        }
      }],
