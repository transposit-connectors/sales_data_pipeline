{
  "v2": {
    "transposit": {
      "dependencies": [{
        "name": "transposit/slack:v2.*.*",
        "alias": "slack",
        "title": "Slack",
        "description": "Authorize this connector if you want to use the Slack API."
      }, {
        "name": "transposit/slack_webhook:v2.*.*",
        "alias": "slack_webhook"
      }, {
        "name": "transposit/aws_s3:v3.*.*",
        "alias": "aws_s3"
      }, {
        "name": "transposit/gcp_bigquery:v1.*.*",
        "alias": "gcp_bigquery"
      }, {
        "name": "transposit/google_sheets:v1.*.*",
        "alias": "google_sheets"
      }],
      "operations": [{
        "id": "webhook",
        "function": {
          "parameters": [{
            "name": "http_event",
            "default": {
              "text": "{   \"query_parameters\": {     \"api_key\": \"XXX\"   },   \"http_method\": \"POST\",   \"parsed_body\": {     \"channel_name\": \"XXX\",     \"user_id\": \"UXXX\",     \"user_name\": \"iggy\",     \"trigger_id\": \"XXX\",     \"team_domain\": \"my_domain\",     \"team_id\": \"TXXX\",     \"text\": \"\",     \"channel_id\": \"DXXX\",     \"command\": \"/test\",     \"token\": \"XXX\",     \"response_url\": \"https://hooks.slack.com/commands/TXXX/XXX/XXX\"   },   \"body\": \"token=XXX&team_id=TXXX&team_domain=my_domain&channel_id=DXXX&channel_name=XXX&user_id=UXXX&user_name=iggy&command=%2Ftest&text=&response_url=https%3A%2F%2Fhooks.slack.com%2Fcommands%2FTXXX%2FXXX%2FXXX&trigger_id=XXX\" }"
            },
            "type": "OBJECT"
          }],
          "js": "webhook.js",
          "lang": "js"
        },
        "access": {
          "requireApiKey": true,
          "webhook": true
        }
      }, {
        "id": "get_slack_message",
        "function": {
          "parameters": [{
            "name": "user",
            "type": "STRING"
          }],
          "js": "get_slack_message.js",
          "lang": "js"
        }
      }, {
        "id": "test_post_message",
        "tql": {
          "tql": "-- This SQL operation is useful to test the message you've\n-- constructed for your webhook. If you provide a Slack\n-- auth in \"Auth & user settings\", it will post a message\n-- to your Slackbot channel.\nSELECT * FROM slack.post_chat_message AS A\nJOIN this.get_slack_message AS B\nON A.$body.blocks = B.blocks\nAND A.$body.text = B.text\nWHERE A.$body.channel = (SELECT user_id from slack.test_auth)"
        }
      }, {
        "id": "pipeline_top",
        "function": {
          "parameters": [{
            "name": "file_url",
            "description": "file location",
            "type": "STRING",
            "required": true
          }],
          "js": "pipeline_top.js",
          "lang": "js"
        }
      }, {
        "id": "load_file",
        "function": {
          "parameters": [{
            "name": "file_url",
            "type": "STRING",
            "required": true
          }],
          "js": "load_file.js",
          "lang": "js"
        }
      }, {
        "id": "get_object",
        "tql": {
          "parameters": [{
            "name": "bucket",
            "type": "STRING",
            "required": true
          }, {
            "name": "key",
            "type": "STRING",
            "required": true
          }],
          "tql": "SELECT * FROM aws_s3.get_object\n  WHERE Bucket=@bucket\n  AND Key=@key"
        }
      }, {
        "id": "execute_dw_query",
        "tql": {
          "tql": "SELECT rows FROM gcp_bigquery.execute_query\n  WHERE projectId='sales-data-pipeline'\nAND $body=(SELECT {\n    'query' : 'SELECT * FROM default.orderdata',\n  })\n\n"
        }
      }, {
        "id": "get_existing_records",
        "function": {
          "js": "lookup_dw_rows.js",
          "lang": "js"
        }
      }, {
        "id": "add_sales_rep",
        "function": {
          "parameters": [{
            "name": "data",
            "type": "ARRAY",
            "required": true
          }],
          "js": "add_sales_rep.js",
          "lang": "js"
        }
      }, {
        "id": "add_inventory",
        "function": {
          "parameters": [{
            "name": "data",
            "type": "ARRAY",
            "required": true
          }],
          "js": "add_inventory.js",
          "lang": "js"
        }
      }, {
        "id": "add_inventory_warning",
        "function": {
          "parameters": [{
            "name": "data",
            "type": "ARRAY",
            "required": true
          }],
          "js": "add_inventory_warning.js",
          "lang": "js"
        }
      }, {
        "id": "generate_region_totals",
        "function": {
          "parameters": [{
            "name": "data",
            "type": "ARRAY"
          }],
          "js": "generate_region_totals.js",
          "lang": "js"
        }
      }, {
        "id": "ship_totals_to_google_sheets",
        "function": {
          "parameters": [{
            "name": "totals",
            "type": "ARRAY",
            "required": true
          }],
          "js": "ship_totals_to_google_sheets.js",
          "lang": "js"
        }
      }, {
        "id": "update_sheet_values",
        "tql": {
          "parameters": [{
            "name": "spreadsheet_id",
            "type": "STRING",
            "required": true
          }, {
            "name": "range",
            "type": "STRING",
            "required": true
          }, {
            "name": "values",
            "type": "ARRAY",
            "required": true
          }],
          "tql": "SELECT * FROM google_sheets.update_sheet_values\n  WHERE range=@range\n  AND spreadsheetId=@spreadsheet_id\n  AND valueInputOption = 'RAW'\n  AND $body=(SELECT {\n    'majorDimension' : 'COLUMNS', -- see https://www.transposit.com/docs/references/connectors/google-sheets-documentation/#update_sheet_values\n    'values' : @values,\n    'range' : @range,\n  })\n"
        }
      }, {
        "id": "update_dataset",
        "tql": {
          "tql": "SELECT * FROM gcp_bigquery.update_dataset\n  WHERE datasetId='<required_string>'\n  AND projectId='<required_string>'\n  AND $body=(SELECT {\n    'access' : [ {\n      'view' : {\n        'datasetId' : '<string>',\n        'tableId' : '<string>',\n        'projectId' : '<string>'\n      },\n      'role' : '<string>',\n      'userByEmail' : '<string>',\n      'domain' : '<string>',\n      'specialGroup' : '<string>',\n      'iamMember' : '<string>',\n      'groupByEmail' : '<string>'\n    } ],\n    'lastModifiedTime' : '<int64>',\n    'creationTime' : '<int64>',\n    'kind' : '<string>',\n    'description' : '<string>',\n    'datasetReference' : {\n      'datasetId' : '<string>',\n      'projectId' : '<string>'\n    },\n    'labels' : {\n      '<string>' : '<string>'\n    },\n    'selfLink' : '<string>',\n    'defaultPartitionExpirationMs' : '<int64>',\n    'etag' : '<string>',\n    'location' : '<string>',\n    'defaultTableExpirationMs' : '<int64>',\n    'id' : '<string>',\n    'friendlyName' : '<string>'\n  })\n\n/*\n * For sample code and reference material, visit\n * https://www.transposit.com/docs/get-started/sql-quickstart\n */"
        }
      }],
      "dataConnections": [{
        "ref": "slack_identify.raw",
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "identity.basic"
            },
            "executionLimits": {
            }
          }
        },
        "title": "Slack",
        "description": "Used to identify you on Slack"
      }, {
        "ref": "slack_bot.raw",
        "overrides": {
          "swagger": {
            "oauthConfig": {
              "scope": "chat:write:bot"
            },
            "executionLimits": {
            }
          }
        }
      }, {
        "ref": "slack.raw"
      }, {
        "ref": "aws_s3",
        "overrides": {
          "aws": {
            "region": "us-west-2",
            "executionLimits": {
            }
          }
        }
      }],
      "javascriptAPIVersion": "3",
      "source": "src",
      "userSettingsDisplay": "GENERATED"
    }
  }
}